{
  "extends": [
    "config:base",
    ":semanticCommitsDisabled"
  ],
  "git-submodules": {
    "enabled": true
  },
  "branchPrefix": "gh-renovate/",
  "allowPostUpgradeCommandTemplating": true,
  "allowedPostUpgradeCommands": [
    "^sed"
  ],
  "platform": "github",
  "prHourlyLimit": 0,
  "onboarding": false,
  "requireConfig": false,
  "autodiscover": true,
  "rebaseWhen": "behind-base-branch",
  "automerge": false,
  "labels": [
    "renovate"
  ],
  "printConfig": true
}
